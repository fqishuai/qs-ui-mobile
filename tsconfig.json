{
  "compilerOptions": {
    "outDir": "./lib",
    "moduleResolution": "node",
    "strict": true,
    "declaration": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "qs-ui-mobile": ["src"],
      "qs-ui-mobile/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"]
}
